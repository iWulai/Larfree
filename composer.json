{
    "name": "iwulai/larfree",
    "description": "make the laravel free!",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "iwulai",
            "email": "i@iwulai.com"
        }
    ],
    "require": {
        "php": "^7.2.13",
        "illuminate/support": "^5.7",
        "illuminate/routing": "^5.7",
        "illuminate/database": "^5.7",
        "illuminate/pagination": "^5.7",
        "illuminate/console": "^5.7",
        "illuminate/validation": "^5.7",
        "illuminate/auth": "^5.7",
        "tymon/jwt-auth": "1.*@rc"
    },
    "require-dev": {
        "itsgoingd/clockwork": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Larfree\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larfree\\Providers\\LarfreeServiceProvider",
                "Larfree\\Providers\\ValidatorServiceProvider"
            ]
        }
    }
}
